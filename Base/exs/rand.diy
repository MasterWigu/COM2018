public void printi(integer s)
public void prints(string s)
integer seed := 100003
const integer mod := 1000000
public void srand(integer s) { 
	if s >= 0 & s < mod then
		seed := s;
};

public integer rand() {
	rand := seed := (seed * 27983 + 149) % mod;
};


public integer entry (integer argc, string *argv, string *envp) {
  printi(rand())
  prints("\n")
  printi(rand())
  prints("\n")
  printi(rand())
  prints("\n")
  entry := 0
};
